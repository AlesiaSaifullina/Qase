<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Regression Suite" parallel="tests" thread-count="2">
    <listeners>
        <listener class-name="tests.base.AnnotationTransformer"/>
        <listener class-name="tests.base.TestListener"/>
    </listeners>
    <test name="Login">
        <classes>
            <class name="tests.LoginTest">
            </class>
        </classes>
    </test>
    <test name="NewAccount">
        <classes>
            <class name="tests.NewProjectTest">
            </class>
        </classes>
    </test>
    <test name="NewSuite">
        <classes>
            <class name="tests.SuiteTest">
            </class>
        </classes>
    </test>
    <test name="NewCase">
        <classes>
            <class name="tests.CaseTest">
            </class>
        </classes>
    </test>
</suite>